{
  "cli": {
    "version": ">= 3.8.1",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "dev:simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true
      }
    },
    "dev:device": {
      "developmentClient": true,
      "autoIncrement": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false
      }
    },
    "preview:simulator": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true
      }
    },
    "preview:device": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}
